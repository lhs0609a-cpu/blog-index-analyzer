# Production Environment Variables for Vercel
# AWS Lightsail 클라우드 서버 사용

# Backend API URL (AWS Lightsail Server with HTTPS)
NEXT_PUBLIC_API_URL=https://api.blrank.co.kr
NEXT_PUBLIC_API_BASE_URL=https://api.blrank.co.kr
API_BASE_URL=https://api.blrank.co.kr

# App Configuration
NEXT_PUBLIC_APP_NAME=블랭크
NEXT_PUBLIC_APP_ENV=production

# Feature Flags
NEXT_PUBLIC_ENABLE_DARK_MODE=true

# 토스페이먼츠 (실제 사용 시 토스페이먼츠에서 발급받은 키로 교체)
NEXT_PUBLIC_TOSS_CLIENT_KEY=
